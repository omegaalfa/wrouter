{
  "name": "omegaalfa/wrouter",
  "type": "library",
  "license": "MIT",
  "description": "This library implements the Trie routing logic",
  "keywords": [
    "router", "routes", "routeTree", "TreeRoute", "NodeRoute",
    "Prefix Trees", "PSR", "PSR-7", "PSR-15", "HTTP",
    "message", "requests", "response", "middleware",
    "Laminas", "Diactoros", "php8"
  ],
  "authors": [
    {
      "name": "wrt",
      "email": "webdesenvolver.agenda@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "laminas/laminas-diactoros": "^2.17 || ^3.0",
    "psr/http-message": "^1.1 || ^2.0",
    "psr/http-server-handler": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "ext-simplexml": "*",
    "ext-libxml": "*"
  },
  "autoload": {
    "psr-4": {
      "Omegaalfa\\Wrouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omegaalfa\\Wrouter\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "phpstan/phpstan": "^1.11",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "scripts": {
    "test": "phpunit",
    "php-cs-fixer-dry-run": "php-cs-fixer fix --dry-run",
    "phpstan": "phpstan analyse src tests --level=max"
  },
  "minimum-stability": "dev"
}
